<!DOCTYPE html>

<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewData["Title"]</title>
	<link rel="stylesheet" asp-append-version="true" href="~/lib/font-awesome/css/all.min.css" />
	<link rel="stylesheet" asp-append-version="true" href="~/lib/bootstrap/css/bootstrap.min.css" />
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
	<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/jqvmap/jqvmap.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/dist/css/adminlte.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/daterangepicker/daterangepicker.css">
	<link rel="stylesheet" href="~/lib/for-admin-lte/plugins/summernote/summernote-bs4.min.css">
</head>
<body>

	<header>
		<partial name="_Navbar" />
		@RenderSection("Header", false)
	</header>
	
	<main role="main" class="pb-3">
		<partial name="_Sidebar" />
		<body class="hold-transition sidebar-mini layout-fixed">
			<div class="wrapper">
				<div class="content-wrapper">
					<partial name="_Notification" />
					@RenderBody()
				</div>
			</div>
		</body>
	</main>
	<footer>
		@RenderSection("Footer", false)
	</footer>
	<aside class="control-sidebar control-sidebar-dark">
		<!-- Control sidebar content goes here -->
	</aside>
	<script src="~/lib/for-admin-lte/plugins/jquery/jquery.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/jquery-ui/jquery-ui.min.js"></script>
	<script>
		$.widget.bridge('uibutton', $.ui.button)
	</script>
	@* <script src="~/lib/for-admin-lte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script> *@
	<script src="~/lib/for-admin-lte/plugins/chart.js/Chart.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/sparklines/sparkline.js"></script>
	<script src="~/lib/for-admin-lte/plugins/jqvmap/jquery.vmap.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
	<script src="~/lib/for-admin-lte/plugins/jquery-knob/jquery.knob.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/moment/moment.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/daterangepicker/daterangepicker.js"></script>
	<script src="~/lib/for-admin-lte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/summernote/summernote-bs4.min.js"></script>
	<script src="~/lib/for-admin-lte/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
	<script src="~/lib/for-admin-lte/dist/js/adminlte.js"></script>
	<script src="~/lib/for-admin-lte/dist/js/pages/dashboard.js"></script>

	<script asp-append-version="true" src="~/lib/jquery/jquery.min.js"></script>
	<script asp-append-version="true" src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
	<script asp-append-version="true" src="~/lib/font-awesome/js/all.min.js"></script>
	@await RenderSectionAsync("Scripts",required:false)
</body>
</html>
